@PhdThesis{rohtua,
  author   = {Hughes,Taylor L.},
  title    = {Time-reversal invariant topological insulators},
  isbn     = {978-1-109-44556-5},
  language = {English},
  note     = {Copyright - Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works; Last updated - 2023-02-23},
  pages    = {239},
  url      = {https://www.proquest.com/dissertations-theses/time-reversal-invariant-topological-insulators/docview/305009703/se-2},
  abstract = {The first examples of topological insulators, the integer quantum Hall effect and the quantum anomalous Hall effect, have been studied for decades. However, in the last few years, a new time-reversal invariant class of topological insulators was theoretically proposed in 2d and 3d. This thesis was an effort to solve two outstanding problems in this new field. The first problem is where to find these new topological insulators in nature. We were able to propose a 2d candidate, HgTe/CdTe quantum wells, which were subsequently studied experimentally. The second problem is how to physically define and distinguish the characteristics of the time-reversal invariant topological insulators. In 2d we proposed fractional charge and charge pumping experiments which distinguish the topological insulator from a trivial band insulator. In 3d we have proposed a novel topological magneto-electric effect which could lead to many beautiful experiments. To analyze the physical properties of the topological insulators we derived a general topological response theory for these systems. As a side effect, this response theory helps to unify a set of topological insulator families into a common framework.},
  file     = {:TI/2024 - PhD thesis, Taylor Hughes.pdf:PDF},
  groups   = {Topology},
  journal  = {ProQuest Dissertations and Theses},
  keywords = {Pure sciences; Chern-Simons theory; Fractional charge; Mercury telluride; Quantum Hall; Topological insulators; Topological quantum field theory; Condensed matter physics; 0611:Condensed matter physics},
  year     = {2009},
}


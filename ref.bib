@Article{Avsar2014,
  author       = {Avsar, A. and Tan, J. Y. and Taychatanapat, T. and Balakrishnan, J. and Koon, G. K. W. and Yeo, Y. and Lahiri, J. and Carvalho, A. and Rodin, A. S. and O'Farrell, E. C. T. and Eda, G. and Castro Neto, A. H. and {\"O}zyilmaz, B.},
  date         = {2014-09-26},
  journaltitle = {Nat. Commun.},
  title        = {Spin-orbit proximity effect in graphene},
  doi          = {10.1038/ncomms5875},
  issn         = {2041-1723},
  number       = {1},
  pages        = {4875},
  volume       = {5},
  abstract     = {The development of spintronics devices relies on efficient generation of spin-polarized currents and their electric-field-controlled manipulation. While observation of exceptionally long spin relaxation lengths makes graphene an intriguing material for spintronics studies, electric field modulation of spin currents is almost impossible due to negligible intrinsic spin-orbit coupling of graphene. In this work, we create an artificial interface between monolayer graphene and few-layer semiconducting tungsten disulphide. In these devices, we observe that graphene acquires spin-orbit coupling up to 17{\thinspace}meV, three orders of magnitude higher than its intrinsic value, without modifying the structure of the graphene. The proximity spin-orbit coupling leads to the spin Hall effect even at room temperature, and opens the door to spin field effect transistors. We show that intrinsic defects in tungsten disulphide play an important role in this proximity effect and that graphene can act as a probe to detect defects in semiconducting surfaces.},
  day          = {26},
  file         = {:moire/2014 - Spin–orbit proximity effect in graphene.pdf:PDF},
  fjournal     = {Nature Communications},
  groups       = {2D untwisted},
  publisher    = {Springer Science and Business Media LLC},
  rights       = {Â© 2014 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
}

